{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "~7.2.0",
    "ext-pcntl": "*",
    "ext-gd": "*",
    "ext-exif": "*",
    "laravel/framework": "^5.5",
    "laravelcollective/html": "^5.3@dev",
    "league/flysystem-aws-s3-v3": "^1.0@dev",
    "spatie/laravel-medialibrary": "^6.0",
    "gloudemans/shoppingcart": "^2.3",
    "graham-campbell/markdown": "^11.0",
    "stripe/stripe-php": "^7.2",
    "gazsp/baum": "^1.1",
    "algolia/algoliasearch-client-php": "^1.9",
    "filp/whoops": "^2.0",
    "predis/predis": "^1.1",
    "fideloper/proxy": "^3.1",
    "guzzlehttp/guzzle": "^6.2",
    "doctrine/dbal": "^2.5",
    "laravel/scout": "^3.0",
    "calcinai/xero-php": "^1.7",
    "laravel/dusk": "^1.1",
    "laravel/tinker": "^1.0",
    "sentry/sentry-laravel": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0",
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "0.9.*",
    "symfony/dom-crawler": "~3.0",
    "symfony/css-selector": "~3.0"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/",
      "Tests\\": "tests/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php",
      "tests/resources/traits",
      "tests/resources/fakes"
    ]
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize",
      "wget https://s3.amazonaws.com/rds-downloads/rds-combined-ca-bundle.pem -P ./storage/certs"
    ],
    "post-update-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}
