{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"type": "project",
	"require": {
		"ext-gd": "*",
		"ext-exif": "*",
		"laravel/framework": "5.2.*",
		"laravelcollective/html": "5.*",
		"league/flysystem-aws-s3-v3": "^1.0@dev",
		"spatie/laravel-medialibrary": "3.*",
		"gloudemans/shoppingcart": "^1.3",
		"graham-campbell/markdown": "^5.0",
		"stripe/stripe-php": "^3.5",
		"almasaeed2010/adminlte": "~2.0",
		"gazsp/baum": "^1.1",
		"spatie/searchindex": "^3.2",
		"algolia/algoliasearch-client-php": "^1.9",
		"filp/whoops": "^2.0",
		"predis/predis": "^1.1",
		"fideloper/proxy": "^3.1",
		"guzzlehttp/guzzle": "^6.2"
	},
	"require-dev": {
		"phpunit/phpunit": "<4.9",
		"fzaninotto/faker": "~1.4",
		"mockery/mockery": "0.9.*",
		"symfony/dom-crawler": "~3.0",
		"symfony/css-selector": "~3.0",
		"tightenco/mailthief": "^0.1.0"
	},
	"autoload": {
		"classmap": [
			"database"
		],
		"psr-4": {
			"App\\": "app/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php",
			"tests/resources/traits"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	}
}
