{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"type": "project",
	"require": {
		"ext-pcntl": "*",
		"ext-gd": "*",
		"ext-exif": "*",
		"laravel/framework": "^5.3",
		"laravelcollective/html": "^5.3@dev",
		"league/flysystem-aws-s3-v3": "^1.0@dev",
		"spatie/laravel-medialibrary": "^4.8.2",
		"gloudemans/shoppingcart": "^1.3",
		"graham-campbell/markdown": "^6.0",
		"stripe/stripe-php": "^3.5",
		"gazsp/baum": "^1.1",
		"algolia/algoliasearch-client-php": "^1.9",
		"filp/whoops": "^2.0",
		"predis/predis": "^1.1",
		"fideloper/proxy": "^3.1",
		"guzzlehttp/guzzle": "^6.2",
		"bugsnag/bugsnag-laravel": "^2.0",
		"doctrine/dbal": "^2.5",
		"laravel/scout": "^1.0",
		"calcinai/xero-php": "^1.4"
	},
	"require-dev": {
		"phpunit/phpunit": "<4.9",
		"fzaninotto/faker": "~1.4",
		"mockery/mockery": "0.9.*",
		"symfony/dom-crawler": "~3.0",
		"symfony/css-selector": "~3.0",
		"tightenco/mailthief": "dev-master"
	},
	"autoload": {
		"classmap": [
			"database"
		],
		"psr-4": {
			"App\\": "app/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php",
			"tests/resources/traits",
			"tests/resources/fakes"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize",
			"wget https://s3.amazonaws.com/rds-downloads/rds-combined-ca-bundle.pem -P ./storage/certs"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	}
}
